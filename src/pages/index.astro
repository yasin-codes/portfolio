<style>
  *:not(nav):not(svg):not(h3):not(#info):not(h3 span):not(a):not(button) {
    opacity: 0;
    transform: translateY(20px);
    transition:
      opacity 0.7s,
      transform 0.7s;
  }

  *:not(nav):not(svg):not(h3):not(#info):not(h3 span):not(a):not(button).animate {
    opacity: 1;
    transform: translateY(0);
  }
</style>

