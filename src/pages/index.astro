<script>
  let lastScrollTop = 0;
  const navbar = document.getElementById("navbar");
  const delta = 5; // Adjust as needed

  window.addEventListener("scroll", function () {
    let scrollTop = window.pageYOffset || document.documentElement.scrollTop;

    if (Math.abs(lastScrollTop - scrollTop) <= delta) return;

    if (scrollTop > lastScrollTop && scrollTop > navbar.offsetHeight) {
      // Scroll down
      navbar.classList.add("-translate-y-[500px]");
    } else {
      // Scroll up or at the top
      navbar.classList.remove("-translate-y-[500px]");
    }

    lastScrollTop = scrollTop;
  });

  // for the modal on contact

  document.addEventListener("DOMContentLoaded", function () {
    var menuButton = document.getElementById("menu-button");
    var modal = document.getElementById("modal1");

    menuButton.addEventListener("click", function () {
      // Toggle the 'hidden' class on modal
      modal.classList.toggle("hidden");
    });
  });

  // for modal contact two

  const button = document.getElementById("buttonCon");
  const modal = document.getElementById("modalCon");

  let isVisible = false;

  button.addEventListener("click", function () {
    if (!isVisible) {
      modal.classList.remove("hidden");
    } else {
      modal.classList.add("hidden");
    }
    isVisible = !isVisible;
  });

  // for hamburger

  // Get references to the elements
  const hamburger = document.getElementById("hamburger");
  const list = document.getElementById("list");

  // Add click event listener to the hamburger label
  hamburger.addEventListener("click", function () {
    // Toggle the 'hidden' class on the list element
    list.classList.toggle("hidden");
  });

  // the background

  const activate = document.getElementById("hamburger");
  const bg = document.getElementById("overlay");

  // Add click event listener to the hamburger label
  activate.addEventListener("click", function () {
    // Toggle the 'hidden' class on the list element
    bg.classList.toggle("hidden");
  });

  // JavaScript to make the div with ID 'overlay' unscrollable
  const overlayDiv = document.getElementById("overlay");
  overlayDiv.style.overflow = "hidden";
  // Optionally, you can disable scrolling using overflow-x and overflow-y separately
  // overlayDiv.style.overflowX = 'hidden';
  // overlayDiv.style.overflowY = 'hidden';

  // for animation
  const elements = document.querySelectorAll("*:not(nav)");

  const checkElementsInView = () => {
    elements.forEach((element) => {
      const rect = element.getBoundingClientRect();
      if (rect.top <= window.innerHeight && rect.bottom >= 0) {
        element.classList.add("animate");
      } else {
        element.classList.remove("animate");
      }
    });
  };

  // Run the function initially to check elements on page load
  checkElementsInView();

  // Add event listener for scroll
  window.addEventListener("scroll", checkElementsInView);

  // h3

  window.addEventListener("scroll", () => {
    const h3 = document.getElementById("my-h3");
    h3.classList.add("hidden");
  });

  // refresh

  window.onbeforeunload = function () {
    window.scrollTo(0, 0);
  };
</script>
